		<window id="homeWin" border="none" width="100%" height="100%" apply="org.irri.iric.portal.LoginController">
			<n:div id="bodyContent"
				style="background-image: url('./assets/img/login_2.png') !important;background-repeat:no-repeat !important;background-position-x: center !important;background-position-y: center !important;background-size: 45% !important;">


				<n:div id="bodyContent1"
					style="margin: 20px;height:800px !important;">

					<n:div class="row"
						style="width:100%; ">
						<n:div class="col-lg-5 col-xl-3" style="margin:5% 15% auto auto;">
							<n:div class="card shadow mb-4"
								style="border-radius:10px;height:95%;">
								<n:div
									class="card-header d-flex justify-content-between align-items-center homeHeader"
									style="background: white!important;border: 0px;margin: auto;margin-top: 40px;">
									<n:h6
										class="text-primary fw-bold m-0">
										Login Account
									</n:h6>
								</n:div>

								<div id="loginfo" visible="false"
									style="margin: auto;margin-right: auto;margin-left: auto; margin-top: 10px;width: 80%;padding-left: 10px;padding-right: 10px;height: 80%;">

									<n:div
										class="card-header d-flex justify-content-between align-items-center homeHeader"
										style="background: white!important; border: 0px; margin: auto;  margin-left: 10%;  margin-top: 40px;">
										<n:h8
											class="text-primary fw-bold m-0">
											<n:b
												style="color: #b33737;font-size: 20px;">
												You are currently login.
											</n:b>
										</n:h8>
									</n:div>
									<hlayout>
										<hlayout
											style="padding:20px;">
											<image id="image_Id" />
										</hlayout>
										<vlayout
											style="padding:10px;">
											<hbox>
												<label value="Name: " />
												<label id="lbl_name" />
											</hbox>
											<hbox>
												<label value="ID: " />
												<label id="lbl_id" />
											</hbox>
											<hbox>
												<label value="Email: " />
												<label id="lbl_email" />
											</hbox>
											<hbox>
												<label
													value="Firstname: " />
												<label
													id="lbl_firstname" />
											</hbox>
											<hbox>
												<label
													value="Lastname: " />
												<label
													id="lbl_lastname" />
											</hbox>
										</vlayout>
									</hlayout>
								</div>
								<div id="logform">
									<n:div class="card-body"
										style="height:420px;margin-left:20px;margin-right:20px;">

										<n:form class="user">
											<n:div class="mb-3">
												<label value="Username" />
												<textbox
													id="txtbox_username" width="100%" style="font-size:12px;" />


											</n:div>
											<n:div class="mb-3">
												<label value="Password" />
												<textbox
													id="txtbox_password" width="100%" style="font-size:12px;"
													type="password" />

											</n:div>
											<n:div class="mb-3">
												<n:div
													class="custom-control custom-checkbox small">
													<n:div
														class="form-check">

													</n:div>
												</n:div>
											</n:div>

											<button id="btn_login"
												style="width: 100%;border-radius: 15px;">
												Login
											</button>
											<n:hr />
											<n:a href="login"
												class="btn btn-primary d-block btn-google btn-user w-100 mb-2"
												role="button"
												style="background: white;
							color: #007bff;
							border-color: #4e9c81;
							border-radius: 10px;">
												<n:i
													class="fab fa-google">
												</n:i>
												Â  Login with Google
											</n:a>
											<n:hr />
											<button id="btn_oauth2_login" style="width: 100%; border-radius: 15px;">
    											Login with Drupal
											</button>
										</n:form>
										<n:div class="text-center">
											<n:a class="small"
												href="forgot-password.html">
												Forgot Password?
											</n:a>
										</n:div>
										<n:div class="text-center">
											<n:a class="small"
												href="register.zul">
												Create an Account!
											</n:a>
										</n:div>
									</n:div>
								</div>
							</n:div>
						</n:div>
					</n:div>



				</n:div>
			</n:div>
		</window>
